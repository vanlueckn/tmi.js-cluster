{
  "name": "tmi.js-cluster",
  "version": "1.0.0-alpha.3",
  "description": "tmi.js-cluster is a scalable cluster for tmi.js. This cluster can have multiple supervisors that can be deployed on multiple servers.",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src --delete-dir-on-start --out-dir dist --copy-files --no-copy-ignored",
    "prepare": "npm run build",
    "watch": "nodemon --exec npm run build --watch src"
  },
  "keywords": [
    "tmi.js",
    "twitch",
    "bot",
    "cluster"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/derpierre65/tmi.js-cluster/"
  },
  "bugs": {
    "url": "https://github.com/derpierre65/tmi.js-cluster/issues/"
  },
  "author": "derpierre65",
  "license": "MIT",
  "dependencies": {
    "deepmerge": "^4.3.1",
    "mysql": "^2.18.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.4",
    "@babel/node": "^7.22.19",
    "@babel/plugin-transform-class-properties": "^7.23.3",
    "@babel/plugin-transform-object-rest-spread": "^7.23.4",
    "@babel/preset-env": "^7.23.4",
    "nodemon": "^3.0.2",
    "redis": "^4.6.12",
    "tmi.js": "^1.9.0-pre.1"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
